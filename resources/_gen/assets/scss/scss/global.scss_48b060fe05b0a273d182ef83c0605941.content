*{box-sizing:border-box}.body-tag{background-color:#ebeef2;font-family:pt sans,sans-serif;line-height:1.5;margin:0;padding:0}.button{border:0;border-radius:.25rem;background:#d42b21;color:#fff;display:inline-block;font-family:system-ui,sans-serif;font-size:1rem;line-height:1.2;white-space:nowrap;text-decoration:none;padding:.25rem .5rem;margin:.25rem;cursor:pointer}.button:hover{opacity:.6}.wrapper{margin:0 auto;max-width:1800px;padding:0 20px}.main-nav-content{align-items:center;display:flex;height:60px;flex:1 1 auto}.logo-wrapper{width:16%}.logo{display:inline-block}.logo-image{display:block;width:40px}.main-menu{border-bottom:1px solid #002147;flex:1 1 auto;height:100%}.main-header{min-height:400px}.hero-message-wrapper{margin:0 0 0 16%}.hero-message{font-size:30px;font-weight:700;line-height:1.3;max-width:60%}.highlight{background-color:#002147;color:#fafafa;padding:0 10px}