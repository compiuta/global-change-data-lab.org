*{box-sizing:border-box}.body-tag{background-color:#ebeef2;color:#002147;font-family:pt sans,sans-serif;line-height:1.5;margin:0;padding:0}.button{border:1px solid #002147;border-radius:8px;background:0 0;color:#002147;display:inline-block;font-size:1rem;line-height:1.2;white-space:nowrap;text-decoration:none;padding:.25rem .5rem;margin:.25rem;cursor:pointer;transition:background,color .2s ease-in-out}.button:hover{background-color:#002147;color:#fafafa}.wrapper{margin:0 auto;max-width:1800px;padding:0 30px}.main-nav-content{align-items:center;display:flex;flex:1 1 auto;padding:20px 0 0}.logo-wrapper{width:16%}.logo{display:inline-block}.logo-image{display:block;width:60px}.main-menu{border-bottom:1px solid #002147;flex:1 1 auto;height:100%;padding:0 0 14px}.nav-link{color:#002147;text-decoration:none;margin:0 20px 0 0}.nav-link:hover{font-weight:700}.main-header{padding:36px 0 56px;position:relative}.hero-message-wrapper{margin:0 0 0 16%}.header-flag{margin:0;text-transform:uppercase}.header-title{font-size:64px;margin:0 0 4px}.hero-message{font-size:26px;line-height:1.3;margin:40px 0;max-width:60%}.highlight{background-color:#d42b21;color:#fafafa;padding:0 10px}.donate-button{background-color:#f8c840;border:0;font-size:24px;font-family:galada,cursive;padding:20px 30px}.donate-button:hover{background-color:#f8c840;color:#002147}.section-title{font-size:30px;margin:20px 0 70px;text-transform:uppercase}.underline{border-bottom:4px solid #d42b21}.mission-section{background-color:#002147;color:#fafafa;padding:24px 0}.mission-list{display:flex;flex-wrap:wrap;list-style:none;margin:0;padding:0}.mission-item{display:flex;padding:0 20px 0 0;margin:0 0 40px;width:50%}.mission-item-title{font-size:24px;line-height:1.2;margin:0}.mission-number{color:#f8c840;font-family:galada,cursive;font-size:160px;line-height:1;margin:0 20px 0 0}@media(max-width:900px){.mission-item{width:100%}}.projects-grid{display:flex;flex-wrap:wrap;justify-content:space-between}.project-item{background-color:#fafafa;border-radius:10px;margin:0 0 20px;overflow:hidden;position:relative;width:49%}.project-item.first{width:100%}.project-link{display:flex;flex-direction:column;min-height:300px;text-decoration:none}.project-description{background-color:rgba(255,255,255,.9);bottom:0;left:0;padding:20px;position:absolute;max-width:50%}.owid-blue-section{background-color:#002147;color:#fafafa;flex:1 1 auto}.owid-red-section{background-color:#d42b21;height:50px}.owid-project-title{font-size:60px;text-align:center}.content-wrapper{align-items:center;display:flex;justify-content:center;text-align:center}.content-image{width:200px}.people-section-inner{display:flex;justify-content:center}.seperator{align-self:center;background-color:#002147;height:200px;margin:0 80px;width:1px}.person-card{display:flex;margin:0 0 20px}.person-image-container{border-radius:50%;height:80px;margin:0 12px 0 0;overflow:hidden;width:80px}.person-image{width:100%}.person-name{font-size:16px;margin:0}.person-title,.person-institute{font-size:12px;margin:0}