*{box-sizing:border-box}.body-tag{background-color:#002147;color:#fafafa;font-family:pt sans,sans-serif;line-height:1.5;margin:0;padding:0}.button{border:1px solid #002147;border-radius:.25rem;background:0 0;color:#002147;display:inline-block;font-size:1rem;line-height:1.2;white-space:nowrap;text-decoration:none;padding:.25rem .5rem;margin:.25rem;cursor:pointer;transition:background,color .2s ease-in-out}.button:hover{background-color:#002147;color:#fafafa}.wrapper{margin:0 auto;max-width:1800px;padding:0 30px}.main-nav-content{align-items:center;display:flex;height:60px;flex:1 1 auto;padding:20px 0 0}.logo-wrapper{width:16%}.logo{display:inline-block}.logo-image{display:block;width:40px}.main-menu{border-bottom:1px solid #f8c840;flex:1 1 auto;height:100%}.nav-link{color:#fafafa;text-decoration:none;margin:0 20px 0 0}.main-header{min-height:500px}.hero-message-wrapper{margin:0 0 0 16%}.header-flag{margin:30px 0 0;text-transform:uppercase}.header-title{font-size:64px;margin:0 0 40px}.hero-message{font-size:26px;font-weight:700;line-height:1.3;max-width:60%}.highlight{background-color:#d42b21;color:#fafafa;padding:0 10px}.donate-button{font-size:24px;height:100px;width:100px;border-radius:50%;display:flex;align-items:center;justify-content:center}.section-title{font-size:30px;margin:20px 0 70px;text-transform:uppercase}.underline{border-bottom:4px solid #d42b21}.mission-section{background-color:#000;color:#fafafa;padding:24px 0}.mission-list{display:flex;flex-wrap:wrap;list-style:none;margin:0;padding:0}.mission-item{display:flex;padding:0 20px 0 0;margin:0 0 40px;width:50%}.mission-item-title{font-size:24px;line-height:1.2;margin:0}.mission-number{color:#f8c840;font-family:galada,cursive;font-size:160px;line-height:1;margin:0 20px 0 0}@media(max-width:900px){.mission-item{width:100%}}